{
  "name": "multipart-form-body-parser",
  "version": "1.0.1",
  "description": "A lightweight, zero-dependency multipart/form-data (MIME type) parser that works in both client and server-side environments (Browser and Node.js)",
  "keywords": [
    "javascript",
    "typescript",
    "multipart",
    "multipart/form-data",
    "parse multipart/form-data",
    "parser",
    "parsing",
    "MIME type",
    "MIME types",
    "media type",
    "media types",
    "form",
    "form payload",
    "form body"
  ],
  "homepage": "https://github.com/rhorge/multipart-form-body-parser",
  "bugs": {
    "url": "https://github.com/rhorge/multipart-form-body-parser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhorge/multipart-form-body-parser.git"
  },
  "license": "MIT",
  "author": "rhorge",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "jest": "^26.1.0",
    "rollup": "^4.22.4",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.1.3",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "module": "dist/index.mjs",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
